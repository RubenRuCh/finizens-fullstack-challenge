{
  "name": "finizens-fullstack-challenge-frontend",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "url": "https://github.com/RubenRuCh/fullstack-challenge"
  },
  "license": "",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.7.0"
  },
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "NODE_ENV=test jest",
    "test:unit:watch": "npx jest --watchAll=true",
    "lint": "tslint src/**/*.ts{,x}",
    "lint:fix": "npx tslint src/**/*.ts{,x} --fix",
    "start:portfolios:frontend": "PORT=3001 react-scripts start",
    "build": "react-scripts build"
  },
  "dependencies": {
    "@types/node": "^14.14.34",
    "@types/uuid": "^8.3.0",
    "@types/uuid-validate": "0.0.1",
    "typescript": "^4.2.3",
    "uuid": "^8.3.2",
    "uuid-validate": "0.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "redux-devtools-extension": "^2.13.9",
    "reselect": "^4.1.5",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.25",
    "@types/react-dom": "^17.0.9",
    "@testing-library/react": "^11.2.7",
    "jest": "^26.6.0",
    "lint-staged": "10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "~1.18.0",
    "tslint-eslint-rules": "^5.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
